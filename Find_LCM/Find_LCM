#include <bits/stdc++.h>
using namespace std;

void main()
{
    int a, b, max;

    cout << "Enter two numbers: ";
    cin >> a >> b; //Take input
    
    // maximum value between a and b is stored in max
 
    if(a>b)
    {
    max=a;
    }
    else
    {
    max=b;
    }

    do
    {
        if (max % a == 0 && max % b == 0)
        {
            cout << "LCM = " << max;
            break;
        }
        else
            ++max;
    } while (true);
}
